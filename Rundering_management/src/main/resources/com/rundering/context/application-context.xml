<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	
	<!-- board.file -->
	<bean id="boardPath" class="java.lang.String">
		<constructor-arg value="${board.upload}" />      
	</bean>
	

	<bean id="testDAOBean" class="com.rundering.dao.TestDAO"
		p:session-ref="sqlSession">
	</bean> 
	<bean id="memberDAOBean" class="com.rundering.dao.MemberDAOBeanImpl"></bean>
	<bean id="testService" class="com.rundering.service.TestServiceImpl"
		p:testDAOBean-ref="testDAOBean">
	</bean>
	
	<bean id="orderGoodsDAO" class="com.rundering.dao.OrderGoodsDAOImpl"
		p:session-ref="sqlSession">
	</bean> 
	
	
	<bean id="orderGoodsService" class="com.rundering.service.OrderGoodsServiceImpl"
		p:orderGoodsDAO-ref="orderGoodsDAO">
	</bean>
	
	
	
</beans>
 