<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	
	<!-- board.file -->
	<bean id="boardPath" class="java.lang.String">
		<constructor-arg value="${board.upload}" />      
	</bean>
	
	<!-- Repository -->

	<bean id="testDAOBean" class="com.rundering.dao.TestDAO"
		p:session-ref="sqlSession"/>
	<bean id="memberDAOBean" class="com.rundering.dao.MemberDAOBeanImpl"
		p:session-ref="sqlSession"/>
	<bean id="menuDAOBean" class="com.rundering.dao.MenuDAOBeanImpl"
		p:session-ref="sqlSession"
	/>
	<bean id="noticeDAO" class="com.rundering.dao.NoticeDAOImpl"
		p:session-ref="sqlSession"	
	 />
	
	<!-- Service -->
	<bean id="testService" class="com.rundering.service.TestServiceImpl"
		p:testDAOBean-ref="testDAOBean"/>
	
	<bean id="noticeService" class="com.rundering.service.NoticeServiceImpl" 
		p:noticeDAO-ref="noticeDAO"
	/>
	<bean id="menuService" class="com.rundering.service.MenuServiceImpl"
		p:menuDAOBean-ref="menuDAOBean"
	/>	
	
	
</beans>
 